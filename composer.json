{
    "name": "stevenmaguire/foundation",
    "description": "Build HTML form elements for Foundation 4 inside Laravel",
    "authors": [
        {
            "name": "Steven Maguire",
            "email": "steven@stevenmaguire.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "illuminate/config": "4.0.x",
        "illuminate/html": "4.0.x"
    },
    "autoload": {
        "psr-0": {
            "Stevenmaguire\\Foundation": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "post-package-install": [
            "php artisan config:publish --path=\"./src/config\" stevenmaguire/foundation"
        ]
    }
}